{
    "version": 2,
    "builds": [
      {
        "src": "build/**",
        "use": "@vercel/static"
      },
      {
        "src": "build/fonts/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/fonts/(.*)",
        "dest": "/build/fonts/$1"
      }
    ]
  }